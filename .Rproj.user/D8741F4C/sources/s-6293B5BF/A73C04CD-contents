                                            from = c("PID4101", "PID4101V", "PID4XXXV", 
                                                     "PID4128V", "PID4134", 
                                                     "PID4132", "PID4130"),
                                            to = c("Control", "Vaccinated (Pfizer)", "Vaccinated (Moderna)", 
                                                     "Vaccinated Convalescent", "Infected 16D", 
                                                     "Infected 30D", "Infected 80D"))
