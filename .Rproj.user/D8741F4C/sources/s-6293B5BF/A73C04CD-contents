      theme(legend.position = "none") +
      ggtitle(paste("Infected PT, clone ID =", lineageID, ";", ncol(lineage.df), "cells\n", unique(VJ.gene))) +
      xlab("Sum CoV2-S1 / S2 / RBD / N Antigen") + ylab("Frequency")
